{
  "name": "webpack-react-app",
  "version": "1.0.0",
  "description": "基于 Webpack 5 + React 19 的现代化前端应用脚手架",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:analyze": "webpack --mode production --analyze",
    "start": "webpack serve --mode development --open",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\"",
    "format:all": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,less,md}\" --ignore-path .prettierignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangjuxu009/webpaceWork.git"
  },
  "keywords": [
    "webpack",
    "react",
    "babel",
    "typescript",
    "前端脚手架"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "bugs": {
    "url": "https://github.com/yangjuxu009/webpaceWork/issues"
  },
  "homepage": "https://github.com/yangjuxu009/webpaceWork#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "babel-loader": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.47.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "html-webpack-plugin": "^5.6.5",
    "less": "^4.4.2",
    "less-loader": "^12.3.0",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
